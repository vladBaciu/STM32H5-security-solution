            "showDevDebugOutput" : "both",
            "serverArgs": ["-m", "0"],
            "gdbPath": "e:/Installs/stmcubeide/STM32CubeIDE_1.15.0/STM32CubeIDE/plugins/com.st.stm32cube.ide.mcu.externaltools.gnu-tools-for-stm32/tools/bin/arm-none-eabi-gdb.exe"
